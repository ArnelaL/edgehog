  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edgehog-device-manager/edgehog"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@craco/craco": "^6.4.0",
    "@formatjs/cli": "^4.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@hookform/resolvers": "^2.8.3",
    "@tanstack/react-table": "^8.9.3",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/js-cookie": "^3.0.1",
    "@types/leaflet": "^1.7.5",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-relay": "^14.1.5",
    "@types/relay-runtime": "^14.1.13",
    "@types/relay-test-utils": "^14.1.0",
    "@types/semver": "^7.3.13",
    "@vitejs/plugin-react": "^4.0.4",
    "apexcharts": "^3.42.0",
    "babel-plugin-relay": "^12.0.0",
    "bootstrap": "^5.1.3",
    "dayjs": "^1.10.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-relay": "^1.8.3",
    "graphql": "^15.7.2",
    "history": "^5.1.0",
    "js-cookie": "^3.0.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-apexcharts": "^1.4.1",
    "react-bootstrap": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.45.4",
    "react-intl": "^5.21.0",
    "react-leaflet": "^3.2.2",
    "react-relay": "^15.0.0",
    "react-router-dom": "^6.0.0-beta.8",
    "react-scripts": "4.0.3",
    "react-select": "^5.7.0",
    "react-select-event": "^5.5.0",
    "relay-compiler": "^15.0.0",
    "relay-runtime": "^15.0.0",
    "relay-test-utils": "^15.0.0",
    "sass": "^1.66.1",
    "semver": "^7.3.8",
    "typescript": "^4.4.4",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-relay-lite": "^0.5.0",
    "vite-tsconfig-paths": "^4.2.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "init": "npm run relay:compile && npm run i18n:compile",
    "start": "vite",
    "prestart": "npm run init",
    "build": "tsc && vite build",
    "prebuild": "npm run init",
    "serve": "vite preview",
    "relay:generate-schema": "cd ../backend && mix absinthe.schema.sdl --schema EdgehogWeb.Schema ../frontend/src/api/schema.graphql",
    "relay:compile": "mkdir -p src/api/__generated__ && relay-compiler",
    "format": "prettier --write src",
    "check-format": "prettier --check src",
    "lint": "eslint --fix src",
    "check-lint": "eslint src",
    "check-types": "tsc --noEmit",
    "precheck-types": "npm run init",
    "jest:test": "TZ=UTC craco test",
    "jest:coverage": "TZ=UTC craco test --coverage",
    "prejest:test": "npm run init",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --ignore '**/*.d.ts' --out-file translations.generated.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "i18n:compile": "formatjs compile-folder src/i18n/langs --ast src/i18n/langs-compiled",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:relay/recommended",
      "prettier"
    ],
    "plugins": [
      "relay"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000/",
  "overrides": {
    "react-error-overlay": "6.0.9"
  }
}
